{
  "name": "variablenamecorrector",
  "publisher": "fireing123",
  "displayName": "Variable Name Corrector",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/fireing123/VariableNameCorrector.git"
  },
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "variablenamecorrector.fixName",
        "title": "변수 정상화"
      },
      {
        "command": "variablenamecorrector.fixLiteralName",
        "title": "상수 정상화"
      },
      {
        "command": "variablenamecorrector.addAbbr",
        "title": "약어 추가"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "variablenamecorrector.fixName",
          "when": "editorTextFocus",
          "group": "navigation"
        },
        {
          "command": "variablenamecorrector.fixLiteralName",
          "when": "editorTextFocus",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "npm run compile && vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.104.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2"
  }
}
